<odoo>
    <record id="copy_address" model="ir.actions.server">
   <field name="name">Copy address to permanent address</field>
   <field name="model_id" ref="st_school_management.model_school_student"/>
   <field name="binding_model_id" ref="st_school_management.model_school_student"/>
   <field name="binding_view_types">list,form</field>
   <field name="state">code</field>
   <field name="code">
       action = records.get_permanent_address()
   </field>
</record>

<!--    <data noupdate="1">-->
       <record id="monthly_attendance" model="ir.cron">
           <field name="name">Monthly Attendance</field>
           <field name="model_id" ref="st_school_management.model_school_student_attendance"/>
           <field name="state">code</field>
           <field name="code">model.send_attendance()</field>
           <field name="user_id" ref="base.user_root"/>
           <field name='interval_number'>31</field>
           <field name='interval_type'>days</field>
           <field name="nextcall"
                  eval="(DateTime.now())"/>
           <field name="priority">1</field>
       </record>
